{
  "name": "@jabedzaman/spotify",
  "type": "module",
  "version": "0.0.1",
  "description": "A TypeScript client for the Spotify Web API",
  "repository": {
    "type": "git",
    "url": "https://github.com/jabedzaman/spotify.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "spotify",
    "api",
    "typescript",
    "music",
    "spotify-api",
    "spotify-web-api"
  ],
  "author": "Jabed Zaman <jabedzaman@outlook.com> (https://github.com/jabedzaman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jabedzaman/spotify/issues"
  },
  "homepage": "https://github.com/jabedzaman/spotify#readme",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/ui": "^4.0.15",
    "dotenv": "^17.2.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}